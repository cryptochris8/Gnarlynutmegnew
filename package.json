{
  "name": "soccer",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "npx tsx index.ts",
    "dev": "npx tsx --watch index.ts",
    "setup": "npm install && echo Setup complete - you can now use npm run start",
    "start:node": "npx tsx index.ts",
    "start:windows": "npx tsx index.ts",
    "start:force-websockets": "HYTOPIA_FORCE_WEBSOCKETS=1 MEDIASOUP_IGNORE_STDERR=1 npx tsx index.ts",
    "dev:windows": "npx tsx --watch index.ts",
    "dev:node": "npx tsx --watch index.ts",
    "check:node": "node --version && echo 'Recommended: Node.js 18.x or 20.x LTS'",
    "install:deps": "npm install",
    "clean:install": "rm -rf node_modules && npm install",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@gltf-transform/cli": "^4.2.0",
    "@hytopia.com/assets": "^0.3.13",
    "hytopia": "^0.10.14",
    "mediasoup": "^3.16.6"
  },
  "trustedDependencies": [
    "mediasoup"
  ]
}
